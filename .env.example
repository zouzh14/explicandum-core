# Google Gemini API Key from https://aistudio.google.com/
GEMINI_API_KEY=your_gemini_api_key_here

# Database URL (SQLite by default)
DATABASE_URL=sqlite:///./explicandum.db

# JWT Secret Key - generate a secure key using: python3 -c "import secrets; print('SECRET_KEY=\"' + secrets.token_urlsafe(32) + '\"')"
SECRET_KEY=your_secure_jwt_secret_key_here

# DeepSeek API Key
DEEPSEEK_API_KEY=your_deepseek_api_key_here

# Resend API Key for Email Verification (from https://resend.com)
RESEND_API_KEY=your_resend_api_key_here

# Mail domain for verification emails
MAIL_DOMAIN=google.com

# Skip actual email sending in development/test (true/false)
SKIP_EMAIL_SENDING=false
